{
    "name": "test-jest",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "jest",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test:watch": "jest --watch",
        "postinstall": "ngcc",
        "compodoc": "compodoc -p tsconfig.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.0.1",
        "@angular/cdk": "^9.1.0",
        "@angular/common": "~9.0.1",
        "@angular/compiler": "~9.0.1",
        "@angular/core": "~9.0.1",
        "@angular/fire": "^6.0.0-rc.1",
        "@angular/forms": "~9.0.1",
        "@angular/localize": "^9.0.4",
        "@angular/material": "^9.1.0",
        "@angular/platform-browser": "~9.0.1",
        "@angular/platform-browser-dynamic": "~9.0.1",
        "@angular/router": "~9.0.1",
        "@briebug/jest-schematic": "^2.1.1",
        "@ng-bootstrap/ng-bootstrap": "^6.0.0",
        "@ngx-gallery/core": "^4.0.3",
        "@ngx-gallery/gallerize": "^4.0.3",
        "@ngx-gallery/lightbox": "^4.0.3",
        "angular-4-data-table": "^0.4.6",
        "bootstrap": "^4.4.1",
        "firebase": "^7.8.0",
        "font-awesome": "^4.7.0",
        "lodash": "^4.17.15",
        "ng2-carouselamos": "^4.1.0",
        "ng2-validation": "^4.2.0",
        "ng5-slider": "^1.2.4",
        "ngx-bootstrap": "^5.5.0",
        "ngx-file-drop": "^8.0.8",
        "ngx-star-rating": "0.0.1",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-builders/jest": "^9.0.0",
        "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
        "@angular-devkit/build-angular": "~0.900.2",
        "@angular/cli": "~9.0.2",
        "@angular/compiler-cli": "~9.0.1",
        "@angular/language-service": "~9.0.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^25.1.2",
        "@types/node": "^12.12.29",
        "codelyzer": "^5.1.2",
        "firebase-tools": "^7.12.1",
        "fuzzy": "^0.1.3",
        "inquirer": "^6.2.2",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^25.1.0",
        "jest-html-reporter": "^2.8.0",
        "jest-preset-angular": "8.0.0",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    },
    "jest": {
        "preset": "jest-preset-angular",
        "roots": [
            "src"
        ],
        "transform": {
            "^.+\\.(ts|js|html)$": "ts-jest"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/src/setup-jest.ts"
        ],
        "reporters": [
            "default",
            [
                "./node_modules/jest-html-reporter",
                {
                    "pageTitle": "Test Report"
                }
            ]
        ],
        "moduleNameMapper": {
            "@app/(.*)": "<rootDir>/src/app/$1",
            "@assets/(.*)": "<rootDir>/src/assets/$1",
            "@core/(.*)": "<rootDir>/src/app/core/$1",
            "@env": "<rootDir>/src/environments/environment",
            "@src/(.*)": "<rootDir>/src/src/$1",
            "@state/(.*)": "<rootDir>/src/app/state/$1"
        },
        "globals": {
            "ts-jest": {
                "tsConfig": "<rootDir>/tsconfig.spec.json",
                "stringifyContentPathRegex": "\\.html$",
                "astTransformers": [
                    "jest-preset-angular/build/InlineFilesTransformer",
                    "jest-preset-angular/build/StripStylesTransformer"
                ]
            }
        }
    }
}
